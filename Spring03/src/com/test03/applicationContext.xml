<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="kim" class="com.test03.Developer">
		<constructor-arg name="name" value="김창진"/>
		<constructor-arg name="salary" value="2500000"/>
		<property name="dept" value="개발"/>
	</bean>
	
	<bean id="lee" class="com.test03.Engineer">
		<constructor-arg index="0" value="이창진"/>
		<constructor-arg index="1" value="3000000"/>
		<property name="dept" value="기술"/>
	</bean>

</beans>
