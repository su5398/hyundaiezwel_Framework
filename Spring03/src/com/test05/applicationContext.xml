<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- C: constructor -->
	<bean id="lee" class="com.test05.Emp" c:name="이창진" c:salary="3000000" />
	<bean id="leecj" class="com.test05.Developer" c:emp-ref="lee" c:dept="개발팀"/>
	
	<!-- p: property -->
	<bean id="kim" class="com.test05.Emp" p:name="김창진" p:salary="3500000"/>
	<bean id="kimcj" class="com.test05.Engineer" p:emp-ref="kim" p:dept="기술팀"/>
</beans>
