<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- id:lee 이창진, 2000000 -->
	<!-- 생성자 -->
	<bean id="lee" class="com.test02.Emp">
		<constructor-arg name="name" value="이창진"/>
		<constructor-arg name="salary" value="2000000"/>
	</bean>
	
	<!-- id:kim 김창진 , 2500000-->
	<!-- setter -->
	<bean id="kim" class="com.test02.Emp">
		<property name="name" value="김창진"/>
		<property name="salary" value="2500000"/>
	</bean>
</beans>
