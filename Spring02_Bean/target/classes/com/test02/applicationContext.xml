<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="kim" class="com.test02.Address">
		<constructor-arg name="name" value="김창진"/>
		<constructor-arg name="phone" value="010-1111-1111"/>
		<constructor-arg name="addr" value="경기도 하남시"/>
	</bean>

	<!-- id:hong 홍길동, 서울시, 010-2222-222 -->
	<bean id="hong" class="com.test02.Address">
		<constructor-arg name="name" value="홍길동"/>
		<constructor-arg name="phone" value="010-2222-2222"/>
		<constructor-arg name="addr" value="서울시"/>
	</bean>
</beans>
